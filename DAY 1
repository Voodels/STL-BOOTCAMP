#include <iostream>
#include <utility>

using namespace std;

int main()
{
    // cout<<"Hello World";
    
    //PAIR 
    
    //combining two differnt datattpe values
    pair<int ,char> PAIRNAME;
        
        
        PAIRNAME.first = 100 ;
        PAIRNAME.second = 'C' ;//single quote because char not string
        cout<<"\n"<<PAIRNAME.first<<  " ";
        cout<<"\n"<<PAIRNAME.second<<endl;
        
        
    pair<char,int> pair1 = make_pair('A',1);
    pair<char,int> pair2 = make_pair('B',2);
    
    cout<<"\n Before swapping ;"<<endl;
    cout<<"content of pair1 =";
    cout<<" "<<pair1.first<<" "<<pair1.second;
    cout<<endl;
    cout<<"content of pair2 =";
    cout<<" "<<pair2.first<<" "<<pair2.second;
    cout<<endl;
    
    //
    pair1.swap(pair2);
    //
    
    
    cout<<"\n after swapping ;"<<endl;
    cout<<"content of pair1 =";
    cout<<" "<<pair1.first<<" "<<pair1.second;
    cout<<endl;
    cout<<"content of pair2 =";
    cout<<" "<<pair2.first<<" "<<pair2.second;
        
    return 0;
}

Q/66. Plus One
-=-=-=-=-=-=-=-=-==-=-=-=-=-=-=-=-=-=-=-=-=-=-==-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=

#include <iostream>
#include<stack>

using namespace std;

int main()
{
    // STACK //- last in first out 
    
    stack<int> st;
    st.push(1);
    st.push(2);
    st.push(3);
    st.push(4);
    st.push(5);
    st.pop();
    st.pop();
    st.pop();
    st.pop();
    st.pop();
    cout<<st.size()<<" "<<st.empty()<<" ";
    
    
    return 0;
}

